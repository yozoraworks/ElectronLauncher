{
  "name": "game-launcher",
  "version": "1.0.0",
  "description": "A simple game launcher built with Electron and React",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"cross-env ELECTRON_START_URL=http://localhost:3000 electron .\" \"npm run react-start\"",
    "react-start": "cross-env BROWSER=none react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "npm run react-build && electron-builder",
    "server": "node server.js",
    "publish": "react-scripts build && electron-builder -p always"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^6.1.7",
    "express": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.game.launcher",
    "productName": "Game Launcher",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "node_modules/**/*"
    ],
    "extends": null,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "signAndEditExecutable": false
    },
    "publish": [
      {
        "provider": "github",
        "owner": "yozoraworks",
        "repo": "ElectronLauncher"
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
